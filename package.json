{
  "name": "knownsec-fed",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "electron": "^1.8.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "package": "electron-packager ./build knownsec-fed --overwrite --all --out ../ --version 1.0.0",    
    "package-mac": "electron-packager ./build knownsec-fed --overwrite --platform=darwin --arch=x64 --out ../ --version 1.0.0",
    "package-win": "electron-packager ./build knownsec-fed --overwrite --platform=win32 --arch=ia32 --prune=true --out ../ --version 1.0.0" 
  },
  "devDependencies": {
    "electron-packager": "^11.1.0"
  }
}
